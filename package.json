{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "render": "mix --mix-config webpack.mix.render.js",
        "module-dev": "cross-env NODE_ENV=development process.env.module=module node_modules/webpack/bin/webpack.js --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
        "module-watch": "cross-env NODE_ENV=development process.env.module=module node_modules/webpack/bin/webpack.js -- --watch --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
        "module-prod": "cross-env NODE_ENV=production process.env.module=module node_modules/webpack/bin/webpack.js --progress --config=node_modules/laravel-mix/setup/webpack.config.js"
    },
    "devDependencies": {
        "axios": "^0.21",
        "cross-env": "^7.0.3",
        "laravel-mix": "^6.0.6",
        "laravel-mix-merge-manifest": "^2.1.0",
        "lodash": "^4.17.19",
        "postcss": "^8.1.14",
        "resolve-url-loader": "^5.0.0",
        "sass": "^1.63.6",
        "sass-loader": "^12.6.0"
    }
}
